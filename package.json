{
  "name": "sandeep-portfolio",
  "version": "2.0.0",
  "private": true,
  "description": "My personal portfolio website built with React, showcasing my projects, skills, and contact information.",
  "author": "Sandeep Kumar Samal (MicroStarkDev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/MicroStarkDev/MicroStarkDev-PortFolio.git"
  },
  "homepage": "https://microstarkdev.github.io/MicroStarkDev-PortFolio",
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "animate.css": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.56.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "gh-pages": "^6.1.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src",
    "fix-deps": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "presets": [
          [
            "babel-preset-react-app",
            false
          ]
        ]
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "eslint-config-react-app": {
      "@typescript-eslint/eslint-plugin": "^6.0.0",
      "@typescript-eslint/parser": "^6.0.0"
    }
  }
}
